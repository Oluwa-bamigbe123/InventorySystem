@model InventorySystem.Models.Equipments

@{ ViewData["Title"] = "Details"; }
@{
    Layout = "";
}
<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InventorySystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>



<h3>Equipment Details</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Equipment Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EquipmentName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EquipmentType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EquipmentType)
        </dd>
        <dt class="col-sm-2">
            Equipment Number
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EquipmentNumber)
        </dd>

    </dl>
    <a asp-action="Index" asp-controller="Equipments">Back to List</a>
</div>
